<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Redux"
                   namespace="Redux.Database.Domain">

  <class name="DbPortal" table="portals">
    <id name="UID" column="UID">
      <generator class="identity" />
    </id>
    <property name="MapID" column="MapID"/>
    <property name="PortalID" column="PortalID"/>
    <property name="PortalX" column="PortalX"/>
    <property name="PortalY" column="PortalY"/>
  </class>

</hibernate-mapping>