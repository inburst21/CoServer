<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Redux"
                   namespace="Redux.Database.Domain">

  <class name="DbPassage" table="passages">
    <id name="UID" column="UID">
      <generator class="identity" />
    </id>
    <property name="EnterMap" column="EnterMap"/>
    <property name="EnterID" column="EnterID"/>
    <property name="ExitMap" column="ExitMap"/>
    <property name="ExitID" column="ExitID"/>
  </class>

</hibernate-mapping>