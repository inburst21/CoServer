<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Redux"
                   namespace="Redux.Database.Domain">

  <class name="DbShopItem" table="shops">
    <id name="UID" column="UID">
      <generator class="identity" />
    </id>
    <property name="ShopID" column="ShopID"/>
    <property name="ItemID" column="ItemID"/>
    <property name="CurrencyType" column="MoneyType"/>
  </class>

</hibernate-mapping>